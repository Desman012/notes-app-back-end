{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console" : 0,
        "eol-last" : 0,
        "import/no-extraneous-dependencies" : 0,
        "consistent-return" : 0,
        "max-len" : 0
    }
}
